{include file="$admingentemplates/admin_top.tpl"}

<font class=red_header>{$header.razdel_name}</font><font class=red_sub_header>&nbsp;|&nbsp;{$header.upm_list}&nbsp;|&nbsp;{$lang.admin_menu.upm_settings}</font><br><br><br>

<form action="{$form.action}" name="form1" method="POST" style="margin:0px">
{$form.form1_hiddens}
<table border=0 cellspacing=1 cellpadding=5>

{section name=num loop=$settings}

<tr bgcolor="#FFFFFF">
  <td width="200px"><font class="main_header_text">{$settings[num].title}: </td>
  <td width="200px">
  {if !$settings[num].iscolor}
    <input type="text" name="settings[{$settings[num].name}]" value="{$settings[num].value}" size="35">
  {else}
    <table border="0" cellpadding="0" cellspacing="0"><tbody><tr>
    <td><input type="text" name="settings[{$settings[num].name}]" value="{$settings[num].value}" size="30">&nbsp;</td>
    <td><table border="0" cellpadding="0" cellspacing="0" width="18"><tbody><tr>
       <td bgcolor="#{if $settings[num].value}{$settings[num].value}{else}FFFFFF{/if}" id="tdcolor{$smarty.section.num.iteration}"><a href="#" onclick="pick('pick{$smarty.section.num.iteration}','tdcolor{$smarty.section.num.iteration}','settings[{$settings[num].name}]');return false;" name="pick{$smarty.section.num.iteration}" id="pick{$smarty.section.num.iteration}"><img src="{$site_root}{$template_root}/images/color.gif" border="0" height="17" width="18"></a></td>
    </tr></tbody></table></td>
    </tr></tbody></table>
  {/if}
  </td>
  <td><font face="Tahoma" style="font-size: 11px">{$settings[num].comment}</td>
</tr>

{/section}

</table><br>
<table><tr>
<td><input type="button" value="{$button.save}" class="button" onClick="javascript: document.form1.submit();"></td>
<td><input type="button" value="{$header.default_settings}" class="button" onclick="{literal}javascript: if(confirm({/literal}'{$header.default_confirm}'{literal})){location.href={/literal}'{$form.default_link}'{literal}}{/literal}"></td>
</tr></table>

</form>

<br><br><font class=red_sub_header>{$header.groups_permissions}</font><br><br><br>

<form action="{$form.action}" name="form2" method="POST" style="margin:0px">
{$form.form2_hiddens}
<table border=0 class="table_main" cellspacing=1 cellpadding=5>

<tr class="table_header">
  <th width="200px"><font class="main_header_text">{$header.groups}</th>
  <th width="100px"><font class="main_header_text">{$header.avenabled}</th>
</tr>

{section name=num loop=$groups}

<tr bgcolor="#FFFFFF">
  <td><font class="main_content_text">{$groups[num].name}</td>
  <td align="center"><input type="checkbox" name="groups[{$groups[num].id}]" value="1" {if $groups[num].check}checked{/if}></td>
</tr>

{/section}

</table><br>
<table><tr>
<td><input type="button" value="{$button.save}" class="button" onClick="javascript: document.form2.submit();"></td>
</tr></table>

</form>

